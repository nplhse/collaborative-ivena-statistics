<div{{ attributes }}>
    <div class="card">
        {% if title %}
            <div class="card-header">
                <h3 class="card-title">{{ title }}</h3>
            </div>
        {% endif %}

        <div class="card-body p-0">
            {% block content %}{% endblock %}
        </div>

        {% if paginator is not null %}
            {{ include('_includes/card_pagination_footer.html.twig') }}
        {% endif %}
    </div>
</div>
