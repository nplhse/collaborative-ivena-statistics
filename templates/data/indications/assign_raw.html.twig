{% extends '_layout_vertical.html.twig' %}

{% block page_content %}
    <h1>Raw-Indikation zuordnen</h1>

    <dl>
        <dt>Hash</dt>
        <dd>{{ raw.hash }}</dd>
    </dl>

    {{ form_start(form) }}
    {{ form_row(form.code) }}
    {{ form_row(form.name) }}
    {{ form_row(form.target) }}
    <button class="btn btn-primary">Speichern</button>
    {{ form_end(form) }}
{% endblock %}
